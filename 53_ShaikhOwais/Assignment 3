#include <stdio.h>

int main() {
    float num1, num2, product;

    // Input two floating-point numbers
    printf("Enter two numbers: ");
    scanf("%f %f", &num1, &num2);

    // Multiply the numbers
    product = num1 * num2;

    // Display the result
    printf("The product of %.2f and %.2f is %.2f\n", num1, num2, product);

    return 0;
}

/* Output
Enter two numbers: 24.7
26.2
The product of 24.70 and 26.20 is 647.14
/*